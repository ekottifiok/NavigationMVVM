<Application x:Class="AmazingNavigation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModel="clr-namespace:AmazingNavigation.MVVM.ViewModels"
             xmlns:views="clr-namespace:AmazingNavigation.MVVM.Views">
    <Application.Resources>
         <DataTemplate DataType="{x:Type TypeName=viewModel:HomeViewModel}">
             <views:HomeView />
         </DataTemplate>
        <DataTemplate DataType="{x:Type TypeName=viewModel:SettingViewModel}">
            <views:SettingView />
        </DataTemplate>
    </Application.Resources>
</Application>
